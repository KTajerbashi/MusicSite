@model IEnumerable<DAL.ViewModels.ViewPlayList>
@{ 
    string Name = "";
}
<ul class="list-group">
    @foreach (var item in Model)
    {
        if (Name != item.PlayListName)
        {
            <li class="list-group-item ItemCountry">
                <a href="@Url.Action("PlayListsPlay","Section",new {id=item.PlayListId})">
                    <span>@item.PlayListName</span>
                    <span>@item.SingerName</span>
                </a>
            </li>
        }
        Name = item.PlayListName;
    }
</ul>
